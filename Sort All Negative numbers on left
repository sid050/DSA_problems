// Iterative C++ program to put all negative first than positive numbers
// Author Siddhanta Das


#include <bits/stdc++.h>
using namespace std;

void  sortnum(int a[], int n)
{
    int i = 0, j = 0;
    for (i = 0; i < n; i++) {
        if (a[i] < 0) {
            if (i != j)
                swap(a[i], a[j]);


            j++;
        }
    }

}
void printarray(int a[], int n)
{
    for  ( int i=0;i<n;i++)
         printf("\t%d",a[i]);

}



int main() {
    int a[] = {2, 4, -10, 13, -7, -60, 52, 8, -19, -5};
    int n = sizeof(a) / sizeof(a[0]);
    sortnum(a ,n);
    printarray(a , n);
 return 0;
}
